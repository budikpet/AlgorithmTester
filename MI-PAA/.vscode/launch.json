    "version": "0.2.0",
    "configurations": [
        {
            "name": "KnapsackSolver",
            "type": "python",
            "request": "launch",
            "args": [
                "--dataFile", "${workspaceFolder}/HomeWork1/data/NR/NR4_inst.dat",
                "-s", "BranchBound"
            ],
            "program": "${workspaceFolder}/HomeWork1/src/knapsackSolver.py",
            // "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Output_generator: files",
            "type": "python",
            "request": "launch",
            "args": [
                "files",
                "-s", "BranchBound",
                "--start-count", "22",
                "${workspaceFolder}/HomeWork1/data/NR", 
                "${workspaceFolder}/HomeWork1/analysisOutput/results"
            ],
            "program": "${workspaceFolder}/HomeWork1/src/output_generator.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Output_generator: file",
            "type": "python",
            "request": "launch",
            "args": [
                "file",
                "-s", "BranchBound",
                "${workspaceFolder}/HomeWork1/data/NR/NR22_inst.dat",
                "${workspaceFolder}/HomeWork1/analysisOutput/results"
            ],
            "program": "${workspaceFolder}/HomeWork1/src/output_generator.py",
            "console": "integratedTerminal"
        }
    ]
}